<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/home2">
    <fragment
        android:id="@+id/home2"
        android:name="com.example.plannova.fragments.Home2Fragment"
        android:label="fragment_home2"
        tools:layout="@layout/fragment_home2" >
        <action
            android:id="@+id/action_home2_to_venueHistoryFragment"
            app:destination="@id/venueHistoryFragment" />
    </fragment>
    <fragment
        android:id="@+id/discover"
        android:name="com.example.plannova.fragments.discoverFragment"
        android:label="fragment_budget"
        tools:layout="@layout/fragment_discover" />
    <fragment
        android:id="@+id/user_info"
        android:name="com.example.plannova.fragments.UserInfoFragment"
        android:label="fragment_user_info"
        tools:layout="@layout/fragment_user_info" />
    <fragment
        android:id="@+id/venueHistoryFragment"
        android:name="com.example.plannova.fragments.venueHistoryFragment"
        android:label="fragment_venue_history"
        tools:layout="@layout/fragment_venue_history" />
</navigation>